#include<bits/stdc++.h>
using namespace std;

void printBinaryOfCharacters(char ch){
    for(int i=31;i>=0;i--){
        int mask=1<<i;
        if((ch & mask)>0){
            cout<<1;
        }
        else{
            cout<<0;
        }
    }
    cout<<endl;
}
int main(){
    char ch='B';
    printBinaryOfCharacters(ch);
}
